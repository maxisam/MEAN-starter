{
  "name": "mean-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:ng": "ng build",
    "build:s": "npm run clear:s && tsc -p src/server && npm run copy-config",
    "build:s:w": "npm run clear:s && tsc -w -p src/server",
    "clear": "npm cache clean --force && rimraf dist/*",
    "clear:ng": "rimraf dist/public",
    "clear:s": "rimraf dist/server",
    "copy-config": "node copy-task.js",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "serve:dev:s": "concurrently \"npm run build:s:w\" \"npm run serve:s\"",
    "serve:dev:s:d": "concurrently \"npm run build:s:w\" \"npm run serve:s:d\"",
    "serve:s": "nodemon dist/server/server.js",
    "serve:s:d": "nodemon --inspect dist/server/server.js",
    "serve:ng:d": "ng serve -pc proxy.conf.json --open",
    "start": "node server/server.js",
    "test:ng": "ng test"
  },
  "private": true,
